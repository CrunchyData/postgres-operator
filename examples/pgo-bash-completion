_pgo_delete_namespace()
{
    last_command="pgo_delete_namespace"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--apiserver-url=")
    flags+=("--debug")
    flags+=("--namespace=")
    two_word_flags+=("-n")
    flags+=("--pgo-ca-cert=")
    flags+=("--pgo-client-cert=")
