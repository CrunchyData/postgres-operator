{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
	"name": "postgres-operator",
        "labels": {
            "name": "postgres-operator",
            "vendor": "crunchydata"
        }
    },
    "spec": {
        "ports": [
            {
                "name": "apiserver",
                "protocol": "TCP",
                "port": $PGO_APISERVER_PORT,
                "targetPort": $PGO_APISERVER_PORT
            },
            {
                "name": "nsqadmin",
                "protocol": "TCP",
                "port": 4171,
                "targetPort": 4171
            },
            {
                "name": "nsqd",
                "protocol": "TCP",
                "port": 4150,
                "targetPort": 4150
            }
        ],
        "selector": {
            "name": "postgres-operator"
        },
        "type": "ClusterIP",
        "sessionAffinity": "None"
    }
}
